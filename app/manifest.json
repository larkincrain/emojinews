{
  "name": "EmojiNews",
  "description": "Gives an emoji analysis of a news article.",
  "version": "1.4",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-311.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    "images/*"
  ],
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],

  "browser_action": {
      "default_title": "EmojiNews",
      "default_icon": "smiling-emoticon-square-face.png"
  },
  "manifest_version": 2
}
