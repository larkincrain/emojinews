{
  "name": "EmojiNews",
  "description": "Gives an emoji analysis of a news article.",
  "version": "1.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "EmojiNews",
      "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}
